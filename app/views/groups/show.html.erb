
<div class="container">
<h1><%= @group.name %></h1>

  <ul class="list-group">
  <% @users.each do |f| %>
      <li class="list-group-item"><%= f.account.name %></li>
    <% end %>
  </ul>

  <div class="mt-3">
  <table><tbody><tr>
  <%= link_to "back", groups_path,class:"button" %> &nbsp; 
  <%= button_to 'Delete Group', @group, method: :delete, data: { confirm: 'Are you sure?' } ,class:"button"%>
    </table></tbody></tr>
    </div>
</div>
